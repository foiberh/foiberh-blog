#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/foiberh/Documents/Frosti/node_modules/.pnpm/ts-jest@29.3.1_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_b605676457a6ce5270d1aa310e5eb47b/node_modules/ts-jest/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/ts-jest@29.3.1_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_b605676457a6ce5270d1aa310e5eb47b/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/foiberh/Documents/Frosti/node_modules/.pnpm/ts-jest@29.3.1_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_b605676457a6ce5270d1aa310e5eb47b/node_modules/ts-jest/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/ts-jest@29.3.1_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_b605676457a6ce5270d1aa310e5eb47b/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
