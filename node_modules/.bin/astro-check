#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/foiberh/Documents/Frosti/node_modules/.pnpm/@astrojs+check@0.9.4_prettier-plugin-astro@0.14.1_prettier@3.5.3_typescript@5.8.2/node_modules/@astrojs/check/dist/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/@astrojs+check@0.9.4_prettier-plugin-astro@0.14.1_prettier@3.5.3_typescript@5.8.2/node_modules/@astrojs/check/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/@astrojs+check@0.9.4_prettier-plugin-astro@0.14.1_prettier@3.5.3_typescript@5.8.2/node_modules/@astrojs/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/@astrojs+check@0.9.4_prettier-plugin-astro@0.14.1_prettier@3.5.3_typescript@5.8.2/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/foiberh/Documents/Frosti/node_modules/.pnpm/@astrojs+check@0.9.4_prettier-plugin-astro@0.14.1_prettier@3.5.3_typescript@5.8.2/node_modules/@astrojs/check/dist/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/@astrojs+check@0.9.4_prettier-plugin-astro@0.14.1_prettier@3.5.3_typescript@5.8.2/node_modules/@astrojs/check/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/@astrojs+check@0.9.4_prettier-plugin-astro@0.14.1_prettier@3.5.3_typescript@5.8.2/node_modules/@astrojs/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/@astrojs+check@0.9.4_prettier-plugin-astro@0.14.1_prettier@3.5.3_typescript@5.8.2/node_modules:/Users/foiberh/Documents/Frosti/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@astrojs/check/dist/bin.js" "$@"
else
  exec node  "$basedir/../@astrojs/check/dist/bin.js" "$@"
fi
